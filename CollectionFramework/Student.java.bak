public class Student 
{
	//interview question
	//create custom set
	String name;
	int rollno;
	public Student(String name,int rollno)
	{
		this.name=name;
		this.rollno=rollno;	
	}
	@Override
	public String toString(){
		return "Student{"+
		        "name="+name+'\'+
				"rollNo="+rollno+
					'}';
	}


	//equals()
    //we can define in basis of rollno 
	//different rollno is possible
   public boolean equals(Object o)
	{
		if(this==0)return true;
		if(o==null || getClass()!=o.getClass())return false;
		Student student =(Student)o;
		return rollno==student.rollno;

	}


   //hashcode()
   //if we inserted any element in set an hash code
   //is generated and it is generated by this function
   //itself if we have two person with 
   //same roll but having different objects
   //but they will generated the same hash code
   @Override
  public int hashCode(){
	return Objects.hash(rollno);
  }
   
	public static void main(String [] args)
	{
		
	}
}
