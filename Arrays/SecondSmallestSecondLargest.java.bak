import java.util.Arrays;
public class SecondSmallestSecondLargest 
{
	public static void main(String[] args) 
	{
		int [] arr={6,5,3,2,1};
		f(arr);
	}
	static void f(int [] arr){
		Arrays.sort(arr);
		int n=arr.length;
		int smallest=arr[1];
		int largest=arr[n-1];
		System.out.println(smallest);
		System.out.println(largest);
	}
}
